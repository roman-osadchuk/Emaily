{
  "name": "emaily",
  "version": "1.0.0",
  "description": "feedback app",
  "main": "index.js",
  "engines": {
    "node": "8.1.1",
    "npm": "5.0.3"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\" \"npm run webhook\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "webhook": "lt -p 5000 -s rtgbdefed"
  },
  "author": "Roman Osadchuk",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.3",
    "localtunnel": "^1.9.0",
    "lodash": "^4.17.10",
    "mongoose": "^5.0.17",
    "nodemon": "^1.17.4",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "path-parser": "^4.2.0",
    "redux-thunk": "^2.2.0",
    "sendgrid": "^5.2.3",
    "stripe": "^6.1.1"
  },
  "devDependencies": {
    "concurrently": "^3.5.1"
  }
}
